{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth.js"
        },
        {
            "src": "/api/songs/(.*)",
            "dest": "/api/songs.js"
        },
        {
            "src": "/api/songs",
            "dest": "/api/songs.js"
        },
        {
            "src": "/api/albums/(.*)",
            "dest": "/api/albums.js"
        },
        {
            "src": "/api/albums",
            "dest": "/api/albums.js"
        },
        {
            "src": "/api/users/(.*)",
            "dest": "/api/users.js"
        },
        {
            "src": "/api/users",
            "dest": "/api/users.js"
        },
        {
            "src": "/api/comments/(.*)",
            "dest": "/api/comments.js"
        },
        {
            "src": "/api/comments",
            "dest": "/api/comments.js"
        },
        {
            "src": "/api/follows/(.*)",
            "dest": "/api/follows.js"
        },
        {
            "src": "/api/banners/(.*)",
            "dest": "/api/banners.js"
        },
        {
            "src": "/api/banners",
            "dest": "/api/banners.js"
        },
        {
            "src": "/api/upload/(.*)",
            "dest": "/api/upload.js"
        },
        {
            "src": "/api/upload",
            "dest": "/api/upload.js"
        },
        {
            "src": "/api/health",
            "dest": "/api/health.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/@(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/user/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/song/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/album/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}